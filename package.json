{
  "name": "stable-coin-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://icerove.github.io/artcoin",
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev-deploy": "near dev-deploy ./contract/art.wasm",
    "deploy": "near deploy --accountId artcoin.testnet --wasmFile ./contract/art.wasm --initFunction new --initArgs {owner_id: 'artcoin.testnet', total_supply: '1000000000000000000000000000000000', ausd_token: 'ausd.artcoin.testnet'}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "near-cli": "^1.5.4"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bn.js": "^5.2.0",
    "bootstrap": "^4.6.0",
    "near-api-js": "^0.38.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3"
  }
}
